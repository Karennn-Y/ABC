LD_HOME         = $(HOME)
CC              = icc
AR              = ar
#SYSTEM_INCL     = -I/tim/tibco/ftl/2.0/include
SYSTEM_INCL     = 
COMMON_INCL     = -I$(LD_HOME)/include
LOCAL_INCL      =
INCLUDE         = $(LOCAL_INCL) $(COMMON_INCL) $(SYSTEM_INCL)
SUBDIRS= sample


LIBNAME         =$(LD_HOME)/lib/

#CFLAGS          = -O3 -no-prec-div -static -xHost -g -c $(INCLUDE)
CFLAGS          = -O3 -g -c $(INCLUDE)
ARFLAGS         = -ruv

.SUFFIXES       : .cc .c .o .cp

.c.o            :
	$(CC) $(CFLAGS) $*.c

.cc.o            :
	$(CC) $(CFLAGS) $*.cc

# OBJS list
OBJS            =

lib             : $(OBJS)
	$(AR) $(ARFLAGS) $(LIBNAME) $(OBJS)

clean           :
	@for i in $(SUBDIRS); do (cd ./$$i && echo $$i: && $(MAKE) $@) done || exit 1

	 rm -f *.o core
