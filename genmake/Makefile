LD_HOME         = $(HOME)/Project
CC              = gcc
UIL             = uil
PROC            = 
FTL_INCL        = /tim/tibco/ftl/2.0/include
COMMON_INCL     = $(LD_HOME)/cominclude
LOCAL_INCL      = $(LD_HOME)/include
INCLUDE         = -I$(LOCAL_INCL) -I$(COMMON_INCL) -I$(FTL_INCL)

FTL_LIB_PATH    = -L/tim/tibco/ftl/2.0/lib
COMMON_LIB_PATH = -L$(LD_HOME)/comlib
LOCAL_LIB_PATH  = -L$(LD_HOME)/lib
LIB_PATH        = $(COMMON_LIB_PATH) $(LOCAL_LIB_PATH) $(FTL_LIB_PATH)

CFLAGS          = -g -c $(INCLUDE)
PROCFLAGS       =
FTL_LIBS        = 
COMMON_LIBS     =
LOCAL_LIBS      =
LIBS            = $(LOCAL_LIBS) $(COMMON_LIBS) $(RDB_LIBS) $(FTL_LIBS)


SUBDIRS=

.SUFFIXES       : .cc .c .o .pc .uil .uid

.pc.c	:
	$(PROC) $(PROCFLAGS) iname=$*
.pc.o	:
	$(PROC) $(PROCFLAGS) iname=$*
	$(CC) $(CFLAGS) $*.cc
.c.o            :
	$(CC) $(CFLAGS) $*.c
.cc.o            :
	$(CC) $(CFLAGS) $*.cc
.uil.uid        :
	$(UIL) -o $*.uid $*.uil

TARGET         = genmake
OBJS           = genmake.o

all  :  $(TARGET)

$(TARGET) : $(OBJS)
	$(CC) -o $@ $(OBJS) $(LIB_PATH) $(LIBS)

install : $(TARGET)
	@strip $(TARGET)
	mv $(TARGET) /data/Flybit/bin
	make clean

clean           :
	 rm -f $(TARGET) *.o core
